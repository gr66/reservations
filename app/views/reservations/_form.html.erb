<%= simple_form_for(@reservation) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :start %>
    <%= f.input :end %>
    <%= f.input :room_type %>
    <%= f.association :user %>
    <%= f.association :room %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
